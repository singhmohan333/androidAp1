<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mohan.myfragments.MainActivity">

    <fragment
        android:id="@+id/fragment"
        android:name="com.example.mohan.myfragments.TopSection"
        android:layout_width="334dp"
        android:layout_height="96dp"
        tools:layout_editor_absoluteY="16dp"
        tools:layout="@layout/top_section"
        tools:ignore="MissingConstraints"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent" />

    <fragment
        android:id="@+id/fragment2"
        android:name="com.example.mohan.myfragments.BottonSection"
        android:layout_width="336dp"
        android:layout_height="351dp"
        tools:ignore="MissingConstraints"
        tools:layout="@layout/bottom_section"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.781"
        android:layout_marginLeft="24dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginRight="24dp"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
